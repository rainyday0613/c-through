AAA;AAGA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA,OAAA;AAAA,QAAA,KAAA,EAAA;AAAA,OAAA;AACA;AAdA,GAzBA;AAyCA,EAAA,OAzCA,qBAyCA;AACA,QAAA,KAAA,MAAA,CAAA,KAAA,CAAA,KAAA,EAAA;AACA,WAAA,IAAA,GAAA,IAAA;AACA,WAAA,QAAA,GAAA,KAAA,MAAA,CAAA,KAAA,CAAA,KAAA;AACA;AACA;AA9CA,CAAA","sourcesContent":["<template>\n  <transition-group name=\"fade\" mode=\"out-in\" tag=\"div\" class=\"main-container\">\n    <div class=\"event-container\" :class=\"{ 'full-container': (full && selected == event.name) }\" v-for=\"event in events\" :key=\"event.name\">\n      <Card\n          :event=\"event\"\n          :buttonText=\"buttonText\"\n          :hideTitle=\"hideTitle\"\n          :hideButton=\"hideButton\"\n          @onOpen=\"onOpen\"\n          @onClose=\"onClose\"\n          @buttonClicked=\"$emit('buttonClicked')\"\n          v-if=\"(!full) || (full && selected == event.name)\"\n      />\n    </div>\n  </transition-group>\n</template>\n\n<script>\nimport Card from './Card';\n\nexport default {\n  components: {\n    Card\n  },\n  props: {\n    events: Array,\n    buttonText: {\n      type: String,\n      default: 'Register'\n    },\n    hideTitle: {\n      type: Boolean,\n      default: false\n    },\n    hideButton: {\n      type: Boolean,\n      default: false\n    }\n  },\n  data() {\n    return {\n      selected: \"\",\n      full: false,\n    }\n  },\n  methods: {\n    onOpen(value) {\n      console.log(\"value : \" + value)\n      if (!this.full) {\n        this.full = true;\n        this.selected = value;\n        this.$router.push({ path: \"/Menu\", query: { event: value } });\n      }\n    },\n    onClose() {\n      setTimeout(() => {\n        this.full = false;\n      }, 400);\n      this.$router.push({ path: \"/Menu\", query: {} });\n    }\n  },\n  created() {\n    if (this.$route.query.event) {\n      this.full = true;\n      this.selected = this.$route.query.event;\n    }\n  }\n}\n</script>\n\n<style scoped>\n.main-container {\n  display: flex;\n  justify-content: center;\n  flex-wrap: wrap;\n}\n.full-container {\n  position: absolute;\n  top: 0;\n  left: 0;\n  height: 100vh;\n  width: 100vw;\n  margin: 0;\n  overflow-y: scroll;\n  overflow: -moz-scrollbars-none;\n  -ms-overflow-style: none;\n  transition: all 1s ease;\n}\n.full-container::-webkit-scrollbar { width: 0 !important }\n\n.fade-move {\n  transition: transform 800ms ease;\n}\n</style>\n"],"sourceRoot":"src/components/Card"}]}