   <td><input type=\"number\" v-model=\"br\" class=\"block\"></td>\n        </tr>\n      </table>\n\n      <table>\n        <tr>\n          <td>\n            <button @click=\"registerOK()\" class=\"block\">확인</button>\n          </td>\n          <td>\n            <button @click=\"$router.push('/')\" type=\"null\" class=\"block\">취소</button>\n          </td>\n        </tr>\n      </table>\n    </div>\n  </form>\n</template>\n\n<script>\nimport axios from 'axios'\n\nexport default {\n  name: 'LoginPage',\n  data() {\n    return {\n      name: '',\n      id: '',\n      pw: '',\n      br: ''\n    }\n  },\n  methods: {\n    registerOK() {\n      console.log('Register Page submit Name : ' + this.name + ' ID : ' + this.id + ' PW : ' + this.pw + ' BR : ' + this.br)\n      const {name, id, pw, br} = this\n      this.$emit('registerOK', {name, id, pw, br})\n    },\n    clickUse(id) {\n      console.log('clickUse : ' + id)\n      axios.post('http://localhost:1234/register/overlap', {id})\n          .then(res => {\n            console.log('res : ' + res.status)\n            if (res.status === 204) {\n              alert('아이디 중복')\n            } else {\n              alert('사용 가능한 아이디')\n            }\n          })\n          .catch(err => {\n            alert(err)\n          })\n    },\n    submit() {\n    }\n  }\n}\n</script>\n\n<style>\n#getTitle {\n  margin-top: 10%;\n}\n\ntable {\n  margin-top: 20px;\n}\n\ntd {\n  padding: 5px;\n}\n\nbutton {\n  padding-right: 40px;\n  padding-left: 40px;\n}\n</style>\n"]}]}