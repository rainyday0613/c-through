ar(--header-color);\n  width: 36px;\n  left: 0;\n  cursor: pointer;\n  transition: all 1s ease;\n\n}\n@mixin pseudoElemMix($prop, $val) {\n  content: \"\";\n  @include menu();\n\n  position: absolute;\n  #{$prop}: $val;\n}\n@mixin rotate($deg) {\n  content: \"\";\n  transform: rotate($deg);\n  top: 0;\n  position: absolute;\n  cursor: pointer;\n}\n.vps-menu-icon {\n  @include menu();\n  position: relative;\n\n  &::before {\n    @include pseudoElemMix(top, -12px);\n  }\n  &::after {\n    @include pseudoElemMix(bottom, -12px);\n  }\n  &-open {\n    position: relative;\n    width: 0;\n    left: -18px;\n    &::before {\n      @include rotate(45deg);\n    }\n    &::after {\n      @include rotate(-45deg);\n    }\n  }\n}\n</style>\n"]}]}